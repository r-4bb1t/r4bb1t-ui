{
  "name": "@r-4bb1t/rabbit-ui",
  "private": false,
  "version": "0.0.37",
  "type": "module",
  "author": "r-4bb1t",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "description": "Rabbit UI: A React component library",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-4bb1t/rabbit-ui.git"
  },
  "scripts": {
    "dev": "vite",
    "build:vite": "tsc && vite build",
    "build:rollup": "rmdir /s /q dist && tsc --project src && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript && rmdir /s /q dist\\types && mkdir dist\\src && move dist\\index.js dist\\src && move dist\\index.d.ts dist\\src",
    "build:type": "tsc --project src",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:ladle": "pnpm ladle build && pnpm ladle preview",
    "dep": "npm publish --access=public"
  },
  "dependencies": {
    "@ladle/react": "^4.0.3",
    "classcat": "^5.0.5",
    "framer-motion": "^11.2.3",
    "lucide": "^0.378.0",
    "lucide-react": "^0.378.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
